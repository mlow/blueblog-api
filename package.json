{
  "name": "blueblog-backend",
  "version": "0.1.0",
  "description": "BlueBlog API",
  "main": "dist/main.js",
  "dependencies": {
    "@graphql-tools/merge": "^6.0.13",
    "@graphql-tools/schema": "^6.0.13",
    "argon2": "^0.26.2",
    "dataloader": "^2.0.0",
    "diff": "^4.0.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.4",
    "graphql-type-json": "^0.3.2",
    "hashids": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.1.0",
    "markdown-it": "^11.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "pg": "^8.3.0",
    "slug": "^3.3.0"
  },
  "devDependencies": {
    "@types/diff": "^4.0.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/markdown-it": "^10.0.1",
    "@types/pg": "^7.14.4",
    "@types/slug": "^0.9.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node -r dotenv/config -r ts-node/register src/main.ts",
    "serve": "nodemon",
    "knex:migrate:latest": "knex --knexfile dist/db/knexfile.js migrate:latest",
    "knex:migrate:up": "knex --knexfile dist/db/knexfile.js migrate:up",
    "knex:migrate:down": "knex --knexfile dist/db/knexfile.js migrate:down"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "",
  "license": "ISC",
  "private": true
}
